{
  "multipart": [
    {
      "when": { "type": "single", "facing": "north" },
      "apply": { "model": "minecraft:block/chest" }
    },
    {
      "when": { "type": "single", "facing": "east" },
      "apply": { "model": "minecraft:block/chest", "y": 90 }
    },
    {
      "when": { "type": "single", "facing": "south" },
      "apply": { "model": "minecraft:block/chest", "y": 180 }
    },
    {
      "when": { "type": "single", "facing": "west" },
